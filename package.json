{
  "name": "make-a-type",
  "displayName": "make a type",
  "repository": "https://github.com/lwkchan/make-a-type",
  "publisher": "lwkchan",
  "description": "",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:make-a-type.transformJSObjectToTypedInterfaceClipboard",
    "onLanguage:typescript",
    "onLanguage:javascript",
    "onLanguage:json"
  ],
  "main": "./dist/extension",
  "contributes": {
    "commands": [
      {
        "command": "make-a-type.transformJSObjectToTypedInterfaceClipboard",
        "title": "Make JS object into TS Type: Clipboard"
      }
    ]
  },
  "scripts": {
    "deploy": "vsce publish --yarn",
    "vscode:prepublish": "webpack --mode production",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js",
    "dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.44.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "@vscode/vsce": "^2.19.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "ts-loader": "^7.0.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0",
    "webpack": "^5.6.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "transform-json-types": "^0.7.0"
  }
}
