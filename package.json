{
  "name": "make-a-type",
  "displayName": "TS type to clipboard",
  "description": "Highlight JS object and generate TS interface in clipboard.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lwkchan/make-a-type"
  },
  "publisher": "lwkchan",
  "version": "1.1.0",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:make-a-type.transformJSObjectToTypedInterfaceClipboard",
    "onLanguage:typescript",
    "onLanguage:javascript",
    "onLanguage:json"
  ],
  "main": "./dist/extension",
  "contributes": {
    "commands": [
      {
        "command": "make-a-type.transformJSObjectToTypedInterfaceClipboard",
        "title": "Make JS object into TS Type: Clipboard"
      }
    ]
  },
  "scripts": {
    "deploy": "vsce publish",
    "vscode:prepublish": "webpack --mode production",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/lodash": "^4.14.195",
    "@types/mocha": "^10.0.10",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.44.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^2.19.0",
    "eslint": "^9.31.0",
    "glob": "^11.0.3",
    "mocha": "^11.7.1",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "transform-json-types": "^0.7.0"
  },
  "packageManager": "npm@10.9.2"
}
